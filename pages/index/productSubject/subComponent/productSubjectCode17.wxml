


<template name="productSubjectCode17">
          <view class='seKillProductContain' wx:if="{{item.code==17}}" style='width:100%;height:{{item.imgWidth}};background:white;'>
              <block wx:for="{{item.data.content}}" wx:key="{{item.data.content[idx2].img_url.CN}}" wx:for-index="idx2">
                <view style='width:50%;height:100%;background:white' bindtap='userLongTapProductAction' data-model='{{item}}'>
                    <image src='{{item.img_url.CN}}' style='width:100%;height:100%;'></image>
                </view>
              </block>
          </view>
</template>